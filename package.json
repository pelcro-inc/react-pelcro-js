  "scripts": {
    "preinstall": "git config core.hooksPath qa/hooks",
    "start": "webpack serve",
    "generateFiles": "SET NODE_ENV=production&&rollup -c",
    "release": "SET NODE_ENV=production&&yarn generateFiles&&yarn publish&&git push&&git push --tags",
    "release:next": "SET NODE_ENV=production&&yarn generateFiles&&yarn publish --tag next&&git push&&git push --tags",
    "lint": "eslint ./src/**/*.js --fix",
    "test": "node ./__tests__/sdkDownloader&&jest --watchAll",
    "test:once": "node ./__tests__/sdkDownloader&&jest"
  },
